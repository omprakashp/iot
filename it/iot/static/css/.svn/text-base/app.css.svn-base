div.container-fluid {
    border: 0em solid Ghostwhite;
    margin-top: 0em;
    padding-bottom: 1em;
    background-color:white;
    padding-top:0em;
}

div.box1 {
    box-sizing: border-box;
    width: 17%;
    height: 100%;
    margin-top: 1em;
    margin-right: 0.2em;
    margin-left: 0.2em;
    border: 0em solid red;
    float: left;
    padding-bottom:0em;
    padding-top:0em;

}
div.box2 {
    box-sizing: border-box;
    width: 61%;
    height: 100%;
    margin-top: 0em;
    padding-top: 0.1em;
    padding-left: 0.5em;
    padding-right: 0.5em;
    border: 0em solid gray;
    float: left;
    padding-bottom:0em;
    height : 100%;
}
div.box3 {
    box-sizing: border-box;
    width: 20%;
    height: 100%;
    margin-left: 1em;
    padding-left: 0.5em;
    padding-right: 0.5em;
    border: 0.1em solid #F8F8FF;
    float: left;
    background-color:#FEFFFF;
    box-shadow: 2px 2px 2px 2px gray;
}

div.container {
    width: 100%;
    border: 0em solid Ghostwhite;
    background-color:#FFFFFF;
    padding-top: 0em;
    padding-bottom: 0em;
}

div.box4 {
    box-sizing: border-box;
    width: 100%;
    height: 5em;
    float: left;
    padding-top: 0.5em;
    
}
div.box5 {
    box-sizing: border-box;
    padding-left:25%;
    width: 30%;
    float: left;
    }
#grad1 {
       background: linear-gradient(to bottom right,#FEFFFF,ghostwhite); }



div.verticalLine {

    position:absolute;
    left:17%;
    top:15%;
    bottom:0%;
  width: 0.1em;
  background-color:lightgray;
  float: left;
  border: 1px ridge lightgray ;
  border-radius: 2px;
  box-shadow: 0.1px 0.1px 0.1px gray;

}

