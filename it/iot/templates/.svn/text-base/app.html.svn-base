      <script type="text/javascript">
	$(document).ready(function() {
	    // Popup for display_url if display_target == 'inline'
	     $( "a.fancy-popup" ).fancybox();

		// Hide help text by default
	    $( "div.display-help-text" ).hide( "fast" );

	    var li_active_class = "li.{{ cobrand.classes.li_active_class|default:'app-crt' }}";
	    var active = $( ".{{ cobrand.classes.app_menu|default:'app-menu-box-cb'}} > ul > " + li_active_class );
		// 124px is the width of each div
	    var idx = $( ".{{ cobrand.classes.app_menu|default:'app-menu-box-cb'}} > ul > li" ).index( active );
	    if( idx >= 3 ) {
		idx -= 3;
	    } else {
		idx = 0;
	    }
	    var start = 124 * idx;

	    $(".{{ cobrand.classes.app_menu|default:'app-menu-box-cb'}}").jMyCarousel({
		btnNext: ".{{ cobrand.classes.app_arrow_right|default:'app-arrow-right' }}",
		btnPrev: ".{{ cobrand.classes.app_arrow_left|default:'app-arrow-left' }}",
		speed: 500,
		visible: '7',
		circular: false,
		start: start,
		scroll: 1,
		eltByElt: true,
		evtStart: 'mouseenter',
		evtStop: 'mouseleave'
	    });
	});


        $(window).load(function() {
            var btn_bg = "{{ cobrand.classes.app_btn_bg|default:'btnplain' }}";
	    $( "input.btnplain" ).each( function() {
		$(this).removeClass( "btnplain" ).addClass( btn_bg );
	    });
            $( "input.btngrad" ).each( function() {
                $(this).removeClass( "btngrad" ).addClass( btn_bg );
            });
	});
      </script>
